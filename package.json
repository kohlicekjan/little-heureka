{
  "name": "little-heureka",
  "version": "1.0.0",
  "description": "This project was developed on the basis of a working interview with Heureka",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --host localhost --port 3000 --inline --mode production",
    "start:dev": "webpack-dev-server --host localhost --port 3000 --watch --hot --inline --mode development",
    "build:production": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "eslint": "eslint ./src/app & exit 0",
    "eslint:fix": "eslint ./src/app --fix & exit 0",
    "eslint:webpack": "eslint ./webpack.config.js --fix & exit 0",
    "stylelint": "stylelint ./src/assets/styles & exit 0",
    "stylelint:fix": "stylelint ./src/assets/styles --fix & exit 0",
    "flow": "flow status & exit 0",
    "flow:flow-typed": "flow-typed install",
    "cypress:open": "cypress open",
    "test": "jest --coverage & cypress run --browser chrome & exit 0",
    "test:unit": "jest --coverage",
    "test:e2e": "cypress run --browser chrome & exit 0",
    "test:lighthouse:audit": "lighthouse http://localhost:3000 -G -A --view --quiet --output-path=./lighthouse-report/report.html",
    "test:lighthouse:security": "lighthouse-security http://localhost:3000 --security --view --quiet --output-path=./lighthouse-report/report.security.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohlicekjan/little-heureka.git"
  },
  "author": "Jan Kohlíček",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kohlicekjan/little-heureka/issues"
  },
  "homepage": "https://github.com/kohlicekjan/little-heureka#readme",
  "devDependencies": {
    "angular-mocks": "^1.7.5",
    "clean-webpack-plugin": "^0.1.19",
    "csp-html-webpack-plugin": "^2.3.0",
    "css-loader": "^1.0.0",
    "cypress": "^3.1.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "lighthouse": "^3.2.1",
    "lighthouse-security": "^0.6.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-rational-order": "^0.0.2",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-pwa-manifest": "^3.7.1",
    "workbox-webpack-plugin": "^3.6.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.0",
    "angular": "1.7.5",
    "angular-resource": "1.7.5",
    "angular-translate": "^2.18.1",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "string-format": "^2.0.0"
  }
}
