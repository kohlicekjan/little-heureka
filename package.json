{
  "name": "little-heureka",
  "version": "1.1.0",
  "description": "This project was developed on the basis of a working interview with Heureka",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=production&& webpack-dev-server --host localhost --port 3000 --inline --mode production",
    "start:dev": "set NODE_ENV=development&& webpack-dev-server --host localhost --port 3000 --watch --hot --inline --mode development",
    "build:prod": "set NODE_ENV=production&& webpack --mode production",
    "build:dev": "set NODE_ENV=development&& webpack --mode development",
    "eslint": "eslint ./src/app & exit 0",
    "eslint:fix": "eslint ./src/app --fix & exit 0",
    "eslint:webpack": "eslint ./webpack.config.js --fix & exit 0",
    "stylelint": "stylelint ./src/assets/styles & exit 0",
    "stylelint:fix": "stylelint ./src/assets/styles --fix & exit 0",
    "cypress:open": "cypress open",
    "test": "jest --coverage & cypress run --browser chrome & exit 0",
    "test:unit": "jest --coverage",
    "test:e2e": "cypress run --browser chrome & exit 0",
    "test:lighthouse:audit": "lighthouse http://localhost:3000 -G -A --view --quiet --output-path=./lighthouse-report/report.html",
    "test:lighthouse:security": "lighthouse-security http://localhost:3000 --security --view --quiet --output-path=./lighthouse-report/report.security.html",
    "electron": "electron .",
    "build:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./dist/favicon.ico --prune=true --out=bin/mac",
    "build:win": "electron-packager . --overwrite --platform=win32 --arch=x64 --icon=./dist/favicon.ico --prune=true --out=bin/win --ignore=\"(doc|src|test|.cache)\" & node installers/windowsInstaller.js",
    "build:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=./dist/favicon.ico --prune=true --out=bin/linux",
    "build:all-platform": "npm run build:mac & npm run build:win & npm run build:linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohlicekjan/little-heureka.git"
  },
  "author": "Jan Kohlíček",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kohlicekjan/little-heureka/issues"
  },
  "homepage": "https://github.com/kohlicekjan/little-heureka#readme",
  "devDependencies": {
    "angular-mocks": "^1.7.6",
    "clean-webpack-plugin": "^1.0.0",
    "csp-html-webpack-plugin": "^2.5.0",
    "css-loader": "^2.1.0",
    "cypress": "^3.1.4",
    "electron": "^4.0.1",
    "electron-packager": "^13.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.7.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "lighthouse": "^4.0.0",
    "lighthouse-security": "^0.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.0",
    "stylelint-config-rational-order": "^0.0.2",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "angular": "^1.7.6",
    "angular-resource": "^1.7.6",
    "angular-translate": "^2.18.1",
    "bootstrap": "^4.2.1",
    "config": "^3.0.1",
    "electron-log": "^2.2.17",
    "i18next": "^13.1.4",
    "jquery": "^3.3.1",
    "string-format": "^2.0.0"
  },
  "main": "main.js"
}
